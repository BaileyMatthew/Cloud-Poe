@model IEnumerable<string>

@{
    ViewData["Title"] = "Contracts";
}

<h1>Contract Management</h1>

<p>
    
</p>

<hr />

<h4>Upload a New Contract</h4>
<div class="row">
    <div class="col-md-6">
        <form asp-action="UploadContract" enctype="multipart/form-data" method="post">
            <div class="form-group">
                <label for="File" class="control-label">Select File</label>
                <input name="File" type="file" class="form-control" required />
            </div>
            <div class="form-group">
                <input type="submit" value="Upload Contract" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<hr />

<h4>Existing Contracts</h4>
<div class="list-group">
    @foreach (var fileName in Model)
    {
        <a href="#" class="list-group-item list-group-item-action">@fileName</a>
    }
</div>